{
  "name": "coding-assignment-node",
  "version": "1.0.0",
  "description": "Coding Assignment [Backend | Node]",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "build": "rimraf dist && babel src --out-dir dist",
    "dev": "nodemon --watch .env --watch src --exec babel-node src",
    "migrate": "babel-node src/migrations/create_table.js",
    "migrate:rollback": "babel-node src/migrations/drop_table.js",
    "migrate:refresh": "babel-node src/migrations/drop_table.js && babel-node src/migrations/create_table.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kritish Dhaubanjar",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/node": "^7.14.7",
    "aws-sdk": "^2.940.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "nodemon": "^2.0.9",
    "rimraf": "^3.0.2"
  }
}
